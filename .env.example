# ============================================
# Elite Web Scraping Configuration
# ============================================
# Copy this file to .env and configure your settings
# cp .env.example .env

# ============================================
# Browser Configuration
# ============================================
HEADLESS=True
BROWSER_TYPE=chromium

# ============================================
# Proxy Configuration
# ============================================
PROXY_ENABLED=False
PROXY_ROTATION_STRATEGY=round_robin
PROXY_MAX_FAILURES=3
PROXY_COOLDOWN_SECONDS=300

# Proxy format: PROXY_N=host:port:username:password:type:protocol
# Example: PROXY_1=proxy.example.com:8080:user:pass:residential:http
# PROXY_CONFIG_FILE=config/proxies.json

# ============================================
# Fingerprint Configuration
# ============================================
FINGERPRINT_REGION=US
FINGERPRINT_ROTATION=True

# ============================================
# Human Behavior Configuration
# ============================================
HUMAN_BEHAVIOR_ENABLED=True
MIN_DELAY=1.0
MAX_DELAY=4.0
SCROLL_DELAY_MIN=0.5
SCROLL_DELAY_MAX=2.0
MOUSE_MOVEMENT_ENABLED=True
SCROLL_ENABLED=True

# ============================================
# Compliance Configuration
# ============================================
RESPECT_ROBOTS_TXT=True
ROBOTS_CACHE_DIR=.cache/robots
ROBOTS_CACHE_TTL=3600

# ============================================
# Retry Configuration
# ============================================
MAX_RETRIES=3
RETRY_DELAY=2.0
RETRY_BACKOFF=2.0

# ============================================
# Timeout Configuration
# ============================================
PAGE_LOAD_TIMEOUT=30000
NAVIGATION_TIMEOUT=30000

# ============================================
# Concurrency Configuration
# ============================================
MAX_CONCURRENT=3

# ============================================
# Output Configuration
# ============================================
OUTPUT_DIR=data
SAVE_IMAGES=True

# ============================================
# Logging Configuration
# ============================================
LOG_LEVEL=INFO
LOG_FILE=


MAX_PAGE=10